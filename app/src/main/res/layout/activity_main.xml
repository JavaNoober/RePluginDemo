<?xml version="1.0" encoding="utf-8"?>
<LinearLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    tools:context="com.xiaoqi.replugindemo.MainActivity">


    <Button
        android:id="@+id/btn_install_outer"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="安装外置插件"/>
    <Button
        android:id="@+id/btn_uninstall_outer"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="卸载外置插件"/>
    <Button
        android:id="@+id/btn_into_inner"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="跳转到内置插件"/>
    <Button
        android:id="@+id/btn_into_outer"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="跳转到外置插件"/>

    <Button
        android:id="@+id/btn_show_plugin"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="显示已安装插件"/>

    <TextView
        android:id="@+id/tv_plugin_list"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"/>
</LinearLayout>
